{
    "name": "paris",
    "author": "Sanil Chawla <sanil@slingshot.fm> (https://sanil.co)",
    "description": "Paris is Slingshot's React design system. It's a collection of reusable components, design tokens, and guidelines that help us build consistent, accessible, and performant user interfaces.",
    "version": "0.4.2",
    "homepage": "https://paris.slingshot.fm",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/slingshot/paris"
    },
    "files": [
        "src",
        "README.md",
        "CHANGELOG.md"
    ],
    "keywords": [
        "react",
        "design system",
        "rsc",
        "server components",
        "ui",
        "components",
        "typescript",
        "nextjs",
        "storybook"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "storybook": "storybook dev -p 6006",
        "build:storybook": "storybook build",
        "create:component": "node ./scripts/createComponent.js",
        "generate:entry": "node ./scripts/generateEntry.js",
        "generate:theme": "pte export ./src/stories/theme/themes.ts LightTheme -o ./public/pte.css",
        "generate:text": "ts-node --esm ./scripts/text.ts",
        "release": "pnpm changeset publish",
        "prepare": "husky install"
    },
    "engines": {
        "pnpm": "^8.x"
    },
    "exports": {
        "./*": "./src/stories/*"
    },
    "dependencies": {
        "@ariakit/react": "^0.2.3",
        "@headlessui/react": "^1.7.14",
        "@radix-ui/react-checkbox": "^1.0.4",
        "clsx": "^1.2.1",
        "pte": "^0.4.9",
        "react-parallax-tilt": "^1.7.144",
        "ts-deepmerge": "^6.0.3"
    },
    "peerDependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-regular-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "react": "^18.x || ^17.x",
        "react-dom": "^18.x || ^17.x",
        "sass": "^1.x",
        "typescript": "^5.0 || ^4.9"
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.1",
        "@ssh/csstypes": "^1.1.0",
        "@ssh/eslint-config": "^1.0.0",
        "@storybook/addon-essentials": "^7.0.18",
        "@storybook/addon-interactions": "^7.0.18",
        "@storybook/addon-links": "^7.0.18",
        "@storybook/addon-mdx-gfm": "^7.0.18",
        "@storybook/addon-styling": "^1.0.8",
        "@storybook/blocks": "^7.0.18",
        "@storybook/manager-api": "^7.0.18",
        "@storybook/nextjs": "^7.0.18",
        "@storybook/react": "^7.0.18",
        "@storybook/testing-library": "^0.1.0",
        "@storybook/theming": "^7.0.18",
        "@types/node": "18.15.11",
        "@types/react": "18.0.31",
        "@types/react-dom": "18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.0.0",
        "autoprefixer": "^10.4.14",
        "change-case": "^4.1.2",
        "csstype": "^3.1.2",
        "eslint": "^8.2.0",
        "eslint-config-next": "13.4.2",
        "eslint-plugin-css": "^0.8.0",
        "eslint-plugin-storybook": "^0.6.12",
        "husky": "^8.0.0",
        "jss": "^10.10.0",
        "jss-preset-default": "^10.10.0",
        "next": "^13.4.2",
        "postcss": "^8.4.23",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.62.1",
        "storybook": "^7.0.18",
        "storybook-dark-mode": "^3.0.0",
        "title-case": "^3.0.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.0.1",
        "type-fest": "^3.10.0",
        "typescript": "^5.0.2"
    }
}
